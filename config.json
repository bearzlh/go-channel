{
  "read_path": [
    {
      "dir": "/var/log/cpslog/",
      "time_format": "Ym/d_H",
      "suffix": ".log",
      "type": "php",
      "on": true,
      "continue": true,
      "index": "log-php-test",
      "pick": "cookie,get"
    },
    {
      "dir": "/usr/local/openresty/nginx/logs/",
      "time_format": "",
      "suffix": "access.log",
      "type": "nginx",
      "on": false,
      "continue": true,
      "index": "log-nginx-test"
    }
  ],
  "env": "test",
  "log": {
    "path": "log",
    "level": "info",
    "format_type": "time",
    "format": "Ym/d_H"
  },
  "factory": {
    "worker_max": 10000,
    "worker_init": 4,
    "on": true
  },
  "msg": {
    "is_batch": true,
    "batch_size": 100,
    "batch_time_second": 3,
    "send_type": "es"
  },
  "monitor": {
    "cpu": 80,
    "memory_restart": 10,
    "sleep_interval_ns": 200,
    "pick_interval": 30,
    "check_interval": 5
  },
  "server_port": ":8081",
  "php_time_window": 2,
  "es": {
    "host": "192.168.0.109:9200",
    "index_format": "Y.m.d",
    "retry": 5,
    "concurrent_post": 20,
    "Storage": "storage"
  },
  "version": "0.13",
  "appid": ""
}
