{
  "read_path": [
    {
      "dir": "/var/log/cpslog/",
      "time_format": "Ym/d_H",
      "suffix": ".log",
      "type": "php",
      "on": true,
      "continue": true,
      "appid": "localhost",
      "pick": "cookie,get,order,user,wechat"
    },
    {
      "dir": "/var/log/cpslog_xd/",
      "time_format": "Ym/d_H",
      "suffix": ".log",
      "type": "xd",
      "on": true,
      "continue": true,
      "appid": "xd",
      "pick": "cookie,get,order,user,wechat"
    }
  ],
  "env": "test",
  "log": {
    "path": "log",
    "level": "info",
    "format_time": "Ym/d_H",
    "format_level": false
  },
  "factory": {
    "worker_max": 10000,
    "worker_init": 4,
    "on": true
  },
  "msg": {
    "is_batch": true,
    "batch_size": 100,
    "batch_time_second": 3,
    "send_type": "es",
    "ip_cache_time": 600,
    "ip_check_interval": 30
  },
  "monitor": {
    "cpu": 80,
    "memory_restart": 10,
    "sleep_interval_ns": 200,
    "pick_interval": 30,
    "check_interval": 5
  },
  "server_port": ":8081",
  "php_time_window": 2,
  "es": {
    "host": "192.168.0.109:9200",
    "index_format": "Y.m.d",
    "retry": 5,
    "concurrent_post": 50,
    "storage": "storage",
    "recover_thread": 20
  },
  "recover": {
    "from": "",
    "to": ""
  },
  "version": "0.23",
  "appid": ""
}
